<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.devzuz.q.maven.wizard.archetypeExtender" name="Archetype Extender" schema="schema/org.devzuz.q.maven.wizard.archetypeExtender.exsd"/>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.devzuz.q.maven.wizard"
            name="%maven2ProjectCreationWizard"/>
      <wizard
            category="org.devzuz.q.maven.wizard"
            class="org.devzuz.q.maven.wizard.projectwizard.Maven2ProjectWizard"
            id="org.devzuz.q.maven.wizard.projectwizard.Maven2ProjectWizard"
            name="%maven2ProjectPOMCreationWizard"
            icon="src/main/resources/icons/Maven_M(16)-TP.gif"
            project="true">
         	<selection class="org.eclipse.core.resources.IProject"/>
         	<selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="org.devzuz.q.maven.wizard.importwizard.Maven2ImportWizard"
            id="org.devzuz.q.maven.wizard.importwizard.Maven2ImportWizard"
            category="org.eclipse.ui.Basic"
            name="%maven2ProjectImportWizard"
            icon="src/main/resources/icons/Maven_M(16)-TP.gif">
            <description>A Wizard for importing Maven 2 Projects</description>
      </wizard>
      <category id="org.eclipse.ui.Basic" name="General"/>
   </extension>
</plugin>
